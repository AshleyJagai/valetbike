<h1>Rental!!! weeee </h1>

<% @current_user = current_user %>
current user: <%= @current_user%>

<% @current_bike = params[:bike] %>
current bike: <%= @current_bike%>
<% @current_station = params[:station] %>
current station: <%= @current_station%>
<%= form_for @rental do |f|%>

 <%= f.hidden_field :user_id, value: @current_user.id%><br>

 <%= f.hidden_field :start_station_id, value: @current_station%><br>

 <%= f.hidden_field :bike_id, value: @current_bike%><br>

 <strong><%= submit_tag "Rent"%> </strong>
 <strong><%= f.submit %></strong>



  <% end %>

  


